    <record id="hotel_restaurant_reservation_0"
            model="hotel.restaurant.reservation">
        <field name="table_nos_ids"
               eval="[(6,0,[ref('hotel_restaurant.hotel_restaurant_tables_table1'),ref('hotel_restaurant.hotel_restaurant_tables_table0')])]"/>
        <field name="room_id" ref="point_of_sale.wall_shelf"/>
        <field eval="(DateTime.today()+ timedelta(days=1))" name="end_date"/>
        <field name="partner_address_id" ref="base.res_partner_4"/>
        <field eval="&quot;draft&quot;" name="state"/>
        <field name="customer_id" ref="base.res_partner_4"/>
        <field eval="&quot;R0/00001&quot;" name="reservation_id"/>
        <field eval="(DateTime.today())" name="start_date"/>
    </record>
