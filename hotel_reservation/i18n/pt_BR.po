#. module: hotel_reservation
#: model:mail.template,body_html:hotel_reservation.mail_template_reservation_reminder_24hrs
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    <p>\n"
"                        <left>Hello <strong>${object.partner_id.name}</"
"strong>.\n"
"                        </left>\n"
"                    </p>\n"
"                    <p>\n"
"                        <left>Greetings from <strong>${object.warehouse_id."
"name}\n"
"                            Hotel</strong>.\n"
"                        </left>\n"
"                    </p>\n"
"                    <p>This email is for reminding you that the Reservation "
"at\n"
"                        the <strong>${object.warehouse_id.name} Hotel</"
"strong>.\n"
"                        % if object.warehouse_id.partner_id.city:\n"
"                        <strong>${object.warehouse_id.partner_id.city}</"
"strong>,\n"
"                        % endif\n"
"                        % if object.warehouse_id.partner_id.country_id."
"name:\n"
"                        <strong>\n"
"                            ${object.warehouse_id.partner_id.country_id.name}"
"</strong>\n"
"                        .\n"
"                        % endif\n"
"                        Your checkin has been confirmed on Date <strong>\n"
"                            ${object.date_order}</strong>.\n"
"                        Please find all the details regarding to the "
"reservation\n"
"                        as listed below:\n"
"                    </p>\n"
"                    <br/>\n"
"                    <strong>Details :</strong>\n"
"                    <br/>\n"
"                    <p style=\"border-left: 1px solid #8e0000; margin-left: "
"30px;\">\n"
"                        &amp;nbsp;&amp;nbsp;Name Of Guest :\n"
"                        <strong>${object.partner_id.name}</strong>\n"
"                        <br/>\n"
"                        &amp;nbsp;&amp;nbsp;Date Of Arrival :\n"
"                        <strong>${object.checkin}</strong>\n"
"                        <br/>\n"
"                        &amp;nbsp;&amp;nbsp;Date Of Departure :\n"
"                        <strong>${object.checkout}</strong>\n"
"                        <br/>\n"
"                        &amp;nbsp;&amp;nbsp;Reservation Number :\n"
"                        <strong>${object.reservation_no}</strong>\n"
"                        <br/>\n"
"                        &amp;nbsp;&amp;nbsp;Number Of Persons : <strong>\n"
"                        ${object.adults}\n"
"                    </strong> Adults and <strong>${object.children}</strong> "
"Childrens\n"
"                        <br/>\n"
"                    </p>\n"
"                    &amp;nbsp;&amp;nbsp;\n"
"                    <strong>Room Rate Per Night:</strong>\n"
"                    <br/>\n"
"                    <br/>\n"
"                    <table border=\"1\" cellpadding=\"4\" style=\"margin-"
"left: 30px;\">\n"
"                        <tr>\n"
"                            <th>Room No</th>\n"
"                            <th>Amount</th>\n"
"                        </tr>\n"
"                        %for line in object.reservation_line_ids:\n"
"                        %for line2 in line.reserve:\n"
"                        <tr>\n"
"                            <td>${line2.product_id.name}</td>\n"
"                            <td>${line2.product_id.list_price}</td>\n"
"                        </tr>\n"
"                        %endfor\n"
"                        %endfor\n"
"                    </table>\n"
"                    <br/>\n"
"                    <p>We would appreciate it if you could let us know in "
"case\n"
"                        there are any changes in your schedule or program.\n"
"                        Please inform us immediately, so that we can refund "
"your\n"
"                        money according to Hotel policies.\n"
"                        We are sure that you will find our services\n"
"                        satisfactory. We look forward to providing you "
"quality\n"
"                        services at our hotel.\n"
"                    </p>\n"
"                    <br/>\n"
"                    <strong>Best Regards</strong>,\n"
"                    <br/>\n"
"                    <br/>\n"
"                    <strong>Reservations Team</strong>\n"
"                    <br/>\n"
"                    [<strong>${object.warehouse_id.name} Hotel</strong>]\n"
"                    <br/>\n"
"                    % if object.warehouse_id.partner_id.street:\n"
"                    <strong>${object.warehouse_id.partner_id.street}</"
"strong>\n"
"                    <br/>\n"
"                    % endif\n"
"                    % if object.warehouse_id.partner_id.city:\n"
"                    <strong>${object.warehouse_id.partner_id.city}</"
"strong>,\n"
"                    <br/>\n"
"                    % endif\n"
"                    % if object.warehouse_id.partner_id.country_id.name:\n"
"                    <strong>\n"
"                        ${object.warehouse_id.partner_id.country_id.name}</"
"strong>\n"
"                    ,\n"
"                    <br/>\n"
"                    % endif\n"
"                    <br/>\n"
"                    % if object.warehouse_id.partner_id.website:\n"
"                    <strong>${object.warehouse_id.partner_id.website}</"
"strong>\n"
"                    <br/>\n"
"                    % endif\n"
"                </p>\n"
"            </div>\n"
"        "
msgstr ""

#. module: hotel_reservation
#: model:mail.template,body_html:hotel_reservation.email_template_hotel_reservation
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    <p>Hello <strong>${object.partner_id.name}</strong>,\n"
"                    </p>\n"
"                    <p>This is to confirm your booking for a room at\n"
"                        the <strong>${object.warehouse_id.name} Hotel</"
"strong>,\n"
"                        % if object.warehouse_id.partner_id.city:\n"
"                        <strong>${object.warehouse_id.partner_id.city}</"
"strong>,\n"
"                        % endif\n"
"                        % if object.warehouse_id.partner_id.country_id."
"name:\n"
"                        <strong>\n"
"                            ${object.warehouse_id.partner_id.country_id.name}"
"</strong>\n"
"                        .\n"
"                        % endif\n"
"                        On behalf of the hotel, we would like to express "
"our\n"
"                        gratitude to you for choosing our services.\n"
"                        Please find all the details regarding the "
"confirmation\n"
"                        of the reservation on Date <strong>\n"
"                            ${format_datetime(object.date_order,tz=user.tz,"
"dt_format='%Y-%m-%d %H:%M:%S')}\n"
"                        </strong> are listed below:\n"
"                    </p>\n"
"                    <strong>Details :</strong>\n"
"                    <br/>\n"
"                    <p style=\"border-left: 1px solid #8e0000; margin-left: "
"30px;\">\n"
"                        Name Of Guest :\n"
"                        <strong>${object.partner_id.name}</strong>\n"
"                        <br/>\n"
"                        Date Of Arrival :\n"
"                        <strong>\n"
"                            ${format_datetime(object.checkin,tz=user.tz, "
"dt_format='%Y-%m-%d %H:%M:%S')}\n"
"                        </strong>\n"
"                        <br/>\n"
"                        Date Of Departure :\n"
"                        <strong>\n"
"                            ${format_datetime(object.checkout,tz=user.tz, "
"dt_format='%Y-%m-%d %H:%M:%S')}\n"
"                        </strong>\n"
"                        <br/>\n"
"                        Reservation Number :\n"
"                        <strong>${object.reservation_no}</strong>\n"
"                        <br/>\n"
"                        Number Of Persons: <strong>${object.adults}</strong> "
"Adults\n"
"                        and <strong>${object.children}</strong> Childrens\n"
"                        <br/>\n"
"                    </p>\n"
"                    <strong>Room Rate Per Night:</strong>\n"
"                    <br/>\n"
"                    <br/>\n"
"                    <table border=\"1\" cellpadding=\"4\" style=\"margin-"
"left: 30px;\">\n"
"                        <tr>\n"
"                            <th>Room No</th>\n"
"                            <th>Amount</th>\n"
"                        </tr>\n"
"                        %for line in object.reservation_line_ids:\n"
"                        %for line2 in line.reserve:\n"
"                        <tr>\n"
"                            <td>${line2.product_id.name}</td>\n"
"                            <td>${line2.product_id.list_price}</td>\n"
"                        </tr>\n"
"                        %endfor\n"
"                        %endfor\n"
"                    </table>\n"
"                    <br/>\n"
"                    <p>We would appreciate it if you could let us know in "
"case\n"
"                        there are any changes in your schedule or program.\n"
"                        In case you require check-in facilities at an "
"earlier\n"
"                        time due to flight changes or any other reasons,\n"
"                        kindly inform us. We are sure that you will find "
"our\n"
"                        services satisfactory. In case you need to cancel\n"
"                        your reservation, please Reply or Call us within 24\n"
"                        hours before the day of the check-in. We look "
"forward\n"
"                        to providing you quality services at our hotel.\n"
"                    </p>\n"
"                    <br/>\n"
"                    <strong>Kind Regards</strong>,\n"
"                    <br/>\n"
"                    <br/>\n"
"                    <strong>Reservations Team</strong>\n"
"                    <br/>\n"
"                    [<strong>${object.warehouse_id.name} Hotel</strong>]\n"
"                    <br/>\n"
"                    % if object.warehouse_id.partner_id.street:\n"
"                    <strong>${object.warehouse_id.partner_id.street}</"
"strong>\n"
"                    <br/>\n"
"                    % endif\n"
"                    % if object.warehouse_id.partner_id.city:\n"
"                    <strong>${object.warehouse_id.partner_id.city}</"
"strong>,\n"
"                    <br/>\n"
"                    % endif\n"
"                    % if object.warehouse_id.partner_id.country_id.name:\n"
"                    <strong>\n"
"                        ${object.warehouse_id.partner_id.country_id.name}</"
"strong>\n"
"                    ,\n"
"                    <br/>\n"
"                    % endif\n"
"                    <br/>\n"
"                    % if object.warehouse_id.partner_id.website:\n"
"                    <strong>${object.warehouse_id.partner_id.website}</"
"strong>\n"
"                    <br/>\n"
"                    % endif\n"
"                </p>\n"
"            </div>\n"
"        "
msgstr ""

